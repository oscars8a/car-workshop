<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record forcecreate="False" id="project_car_workshop_stage_on_hold" model="project.task.type">
            <field name="sequence">1</field>
            <field name="name">On Hold</field>
        </record>

        <record forcecreate="False" id="project_car_workshop_stage_in_progress" model="project.task.type">
            <field name="sequence">3</field>
            <field name="name">In Progress</field>
        </record>

        <record forcecreate="False" id="project_car_workshop_stage_collection_customer" model="project.task.type">
            <field name="sequence">4</field>
            <field name="name">Collection Customer</field>
        </record>

        <record id="project_car_workshop_1" model="project.project">
            <field name="date_start" eval="time.strftime('%Y-%m-01 10:00:00')"/>
            <field name="name">Repair and Revision</field>
            <field name="color">3</field>
            <field name="car_work">True</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="type_ids" eval="[(4, ref('car_workshop.project_car_workshop_stage_on_hold')),
            (4, ref('car_workshop.project_car_workshop_stage_in_progress')),
            (4, ref('car_workshop.project_car_workshop_stage_collection_customer'))]"/>
            <field name="partner_id" ref="base.partner_root"/>
            <field name="privacy_visibility">portal</field>
        </record>
    </data>
</odoo>
